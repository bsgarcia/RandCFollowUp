clear all

i = 0;
j = 0;

for midpoint = [1:9]./10
    i = i + 1;
 
    x = linspace(0, 1, 12);
    y(i,:) = logfun(x', midpoint, temp);

    [xout(i), yout] = intersections(x, y, x, ones(1, 12)*0.5);
end

figure('Renderer', 'painters', 'visible', 'on');

yy = mean(y);
xout2 = 


function p = logfun(x, midpoint, temp)
    p = 1./(1+exp(-temp.*(midpoint(1)-x)));
end